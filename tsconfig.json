{
  "compilerOptions": {
    "target": "ES2022",
    "module": "CommonJS",
    "lib": ["esnext", "DOM"],
    "sourceMap": true,
    "outDir": "./dist",
    "removeComments": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,    
  }, "include": [
    "./src"
  ]
}